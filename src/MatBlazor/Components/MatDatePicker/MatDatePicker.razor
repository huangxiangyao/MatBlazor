@namespace MatBlazor
@inherits BaseMatDatePicker


<MatTextFieldView
    RefBack="@RefBack"
    Class="@($"{Class} {FieldClass}")"
    Style="@Style"
    Icon="@MatIconNames.Calendar_today"
    IconTrailing="true"
    Label="@Label"
    Dense="@Dense"
    HelperText="@HelperText"
    Outlined="@Outlined"
    FullWidth="@FullWidth"
    PlaceHolder="@PlaceHolder"
    Required="@Required"
    Disabled="@Disabled"
    ReadOnly="@ReadOnly"
    Attributes="@Attributes"
    Id="@Id"
    @ref="TextFieldRef"
    >
</MatTextFieldView>